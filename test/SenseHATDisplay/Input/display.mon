
monitor displayTest
{
	import "SenseHATPlugin" as senseHat;

	action onload()
	{
		integer ret := 0;

		ret := senseHat.setPixel(5, 4, 255, 255, 0);
		ret := senseHat.setPixel(5, 5, 255, 0, 0);
		ret := senseHat.setPixel(5, 6, 0, 255, 0);
		ret := senseHat.setPixel(5, 7, 0, 0, 255);

		log "Finished loading";
	}
}
